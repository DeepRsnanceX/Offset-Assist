{
	"geode": "4.9.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"settings": {
		"renders-path": {
			"type": "folder",
			"name": "Renders Folder",
			"description": "Where to save the Icon Renders to. Defaults to this mod's config directory.",
			"default": "{mod_config_dir}/renders"
		},
		"bg-color": {
			"type": "rgba",
			"name": "Render Background Color",
			"description": "Defines the BG color to use for the renders. Defaults to transparent (which is best, honestly)",
			"default": [0, 0, 0, 0]
		}
	},
	"id": "saritahhh.offset_assist",
	"name": "Icon Construct",
	"version": "v1.0.0-beta.2",
	"developer": "Saritahhh",
	"description": "All-in-one Utils for Icon Creators!",
	"links": {
		"source": "https://github.com/DeepRsnanceX/Geode-Icon-Construct"
	},
	"dependencies": {
		"geode.node-ids": ">=v1.20.0",
		"hiimjustin000.more_icons": {
            "version": ">=v1.14.0",
            "importance": "required"
        }
	}
}
